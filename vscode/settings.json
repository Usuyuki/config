{
  "workbench.colorTheme": "Atom One Dark",
  "cSpell.userWords": [
    "cabo",
    "cabocha",
    "cha",
    "csrf",
    "directmessage",
    "endcomponent",
    "enderror",
    "endfor",
    "endforeach",
    "endisset",
    "endphp",
    "endsection",
    "endslot",
    "explain",
    "explain nowja",
    "explain oldja",
    "futaketa",
    "goketa",
    "goketame",
    "hitoketa",
    "hitoketa list",
    "kan",
    "kan suuzi",
    "kotonoha",
    "Larastan",
    "Laravel",
    "list",
    "magunitude",
    "nowja",
    "oldja",
    "openapi",
    "privacypolicy",
    "pydub",
    "pygame",
    "Restapp",
    "sanketa",
    "shinonome",
    "skyblue",
    "sqlite",
    "suuzi",
    "swiper",
    "ulab",
    "Usuyuki",
    "utsunomiya",
    "uucircle",
    "UUID",
    "yonketa"
  ],
  "git.enableSmartCommit": true,
  "workbench.tree.indent": 20,
  "workbench.iconTheme": "simple-icons",
  "workbench.startupEditor": "welcomePage",
  "editor.formatOnSave": true,
  "liveServer.settings.donotShowInfoMsg": true,

  "[html]": {
    "editor.defaultFormatter": "apility.beautify-blade"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jupyter.interactiveWindowMode": "perFile",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "intelephense.stubs": [
    "apache",
    "bcmath",
    "bz2",
    "calendar",
    "com_dotnet",
    "Core",
    "ctype",
    "curl",
    "date",
    "dba",
    "dom",
